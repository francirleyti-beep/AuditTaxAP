# Configurações da SEFAZ AP
# URL da página de consulta do Memorial
SEFAZ_URL=http://www.sefaz.ap.gov.br/EMISSAO/memorial.php

# Timeout em segundos para operações do Selenium
SEFAZ_TIMEOUT=30

# Rodar navegador em modo invisível (True/False)
# Recomendado True para servidores, False para debug
HEADLESS_MODE=False

# Tolerância para diferenças monetárias (em Reais)
# Ex: 0.05 aceita diferenças de até 5 centavos
AUDIT_TOLERANCE=0.05

# Nível de Log (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
